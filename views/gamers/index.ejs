<%- include('../partials/header') %>
<table id="list">
    <thead>
      <tr>
        <th>Title</th>
        <th>Release<br>Year</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
        <% games.backlog.forEach(function(b) { %>
          <tr>
            <%b.games.forEach(function(g){ %> 
            <td><%= g.title %></td>
            <td><%= g.released %></td>
            <td><a href="/games/<%= g._id %>">DETAILS</a></td>
          </tr>
        <% }); %> 
          <% }); %>
        </tbody>
      </table>
    </table>
<%- include('../partials/footer') %>